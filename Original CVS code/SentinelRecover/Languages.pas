const aDescLanguages: array[TLanguages] of string = ('English','Italian','French','German','Spanish','Japanese','Polish','Russian','Portuguese','Bulgarian','Indonesian','Azeri','Dutch','Hebrew','Galician');

var CurLang : TLanguages;
